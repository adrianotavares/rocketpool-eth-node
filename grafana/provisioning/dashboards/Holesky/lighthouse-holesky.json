{
  "id": null,
  "uid": "lighthouse-holesky-monitoring",
  "title": "Lighthouse Holesky Testnet Monitoring",
  "tags": ["ethereum", "lighthouse", "consensus", "holesky", "testnet"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
        {
      "id": 1,
      "type": "stat",
      "title": "Service Status",
      "targets": [
        {
          "expr": "up{job=\"lighthouse-holesky\"}",
          "refId": "A",
          "instant": true,
          "intervalFactor": 1,
          "maxDataPoints": 1
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "short",
          "min": 0,
          "max": 1
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Beacon Head Slot",
      "targets": [
        {
          "expr": "beacon_head_slot{job=\"lighthouse-holesky\"}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1000000
              },
              {
                "color": "green",
                "value": 4000000
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Active Validators",
      "targets": [
        {
          "expr": "beacon_current_active_validators{job=\"lighthouse-holesky\"}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100000
              },
              {
                "color": "green",
                "value": 1000000
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "HTTP API Status",
      "targets": [
        {
          "expr": "up{instance=\"eth2-holesky:5054\"}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Slot Progress",
      "targets": [
        {
          "expr": "beacon_head_slot{job=\"lighthouse-holesky\"}",
          "refId": "A",
          "legendFormat": "Current Slot"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Epoch Progress",
      "targets": [
        {
